pipeline {
    agent any
    stages {
        stage("cloning my git"){
        steps {
            git(
                url:"git@github.com:BHARATHGNANASEKARAN/githubrepo.git",
                 branch:"main"
                 crendentialid:"Bharath@27"
                  timeout:10 )
        }
    }
    stage("build"){
        steps {
            bat"mvn --version"
            bat"mvn package"
        }
    }
}
}
